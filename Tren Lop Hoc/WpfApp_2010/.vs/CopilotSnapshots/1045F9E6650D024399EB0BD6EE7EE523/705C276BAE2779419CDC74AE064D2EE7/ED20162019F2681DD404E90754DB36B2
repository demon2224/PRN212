        public admin()
        {
            InitializeComponent();
        }

        private void btnBack_Click(object sender, RoutedEventArgs e)
        {
            MainWindow mainWindow = new MainWindow();
            mainWindow.Show();
            this.Close(); 
        }

        private void btnFunc1_Click(object sender, RoutedEventArgs e)
        {
            MainFrame.Navigate(new afunc1());
        }

        private void btnFunc2_Click(object sender, RoutedEventArgs e)
        {
            MainFrame.Navigate(new afunc2());
        }

        private void btnFunc3_Click(object sender, RoutedEventArgs e)
        {
            MainFrame.Navigate(new afunc3());
        }
